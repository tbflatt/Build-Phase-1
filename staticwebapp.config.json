{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "clientIdSettingName": "AzureActiveDirectory__ClientId",
          "clientSecretSettingName": "AzureActiveDirectory__ClientSecret",
          "issuer": "https://login.microsoftonline.com/YOUR_TENANT_ID/v2.0"
        }
      }
