	<!-- JavaScript for Zoom and Pan Functionality -->
	<script>
		let scale = 1;
		const iframe = document.getElementById('canvaIframe');
		const container = document.getElementById('zoomContainer');

		// Zoom in/out with mouse wheel
		container.addEventListener('wheel', function(event) {
			event.preventDefault();
			scale += event.deltaY * -0.001;
			scale = Math.min(Math.max(.5, scale), 3); // Limit zoom scale
			iframe.style.transform = `scale(${scale})`;
		});

		// Pan with mouse drag
		let isPanning = false;
		let startX, startY, translateX = 0, translateY = 0;

		container.addEventListener('mousedown', function(event) {
			isPanning = true;
			startX = event.clientX - translateX;
			startY = event.clientY - translateY;
		});

		container.addEventListener('mousemove', function(event) {
			if (!isPanning) return;
			translateX = event.clientX - startX;
			translateY = event.clientY - startY;
			iframe.style.transform = `scale(${scale}) translate(${translateX}px, ${translateY}px)`;
		});

		container.addEventListener('mouseup', function() {
			isPanning = false;
		});

		container.addEventListener('mouseleave', function() {
			isPanning = false;
		});
	</script>

